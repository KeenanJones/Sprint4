<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="324.0" prefWidth="579.0" xmlns="http://javafx.com/javafx/8.0.181" xmlns:fx="http://javafx.com/fxml/1" fx:controller="planSelectionView.PlanSelectionViewController">
   <center>
      <VBox prefHeight="249.0" prefWidth="268.0" BorderPane.alignment="CENTER">
         <children>
            <BorderPane prefHeight="128.0" prefWidth="280.0">
               <top>
                  <Label text="Plan Templates" BorderPane.alignment="CENTER" />
               </top>
               <center>
                  <ScrollPane fitToWidth="true" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                     <content>
                        <ListView fx:id="planTemplateList" maxWidth="1.7976931348623157E308" onMouseClicked="#openPlanTemplate" prefHeight="200.0" />
                     </content>
                  </ScrollPane>
               </center>
            </BorderPane>
            <BorderPane prefHeight="200.0" prefWidth="200.0">
               <top>
                  <Label text="Department Plans" BorderPane.alignment="CENTER" />
               </top>
               <center>
                  <ScrollPane fitToWidth="true" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                     <content>
                        <ListView fx:id="departmentPlanList" maxWidth="1.7976931348623157E308" onMouseClicked="#openPlan" prefHeight="200.0" />
                     </content>
                  </ScrollPane>
               </center>
            </BorderPane>
         </children>
      </VBox>
   </center>
   <top>
      <HBox prefHeight="38.0" prefWidth="524.0" BorderPane.alignment="CENTER">
         <children>
            <Separator prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <Label prefHeight="25.0" prefWidth="100.0" text="Select Plan" />
            <Separator prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <Button mnemonicParsing="false" onAction="#Logout" prefWidth="68.0" text="Logout" />
         </children>
      </HBox>
   </top>
</BorderPane>
